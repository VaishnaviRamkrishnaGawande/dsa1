package com.demo.hashing;

public class Hashing {
	Node head;
      class Node{
    	  int data;
    	  Node next;
		public Node(int data, Node next) {
			super();
			this.data = data;
			this.next = null;
		}
      }
      
		public Hashing(Node head) {
		super();
		this.head = null;
	}

		public void add(int val)
		{
			Node newnode=new Node(val);
			{
				if(head==null)
				{
					head=newnode;
				}else{
					newnode.next=head;
					head=newnode;
				}
			}
		}
    	  
      }
}
