package com.demo.Queue;

public class Queue {
	Node front,rear;
	class Node
	{
		int data;
		Node next;
		public Node(int data) {
			super();
			this.data = data;
			next=null;
		}	
	}
	public Queue() {
		super();
		front=null;
		rear=null;
	}
	
	public boolean isEmpty()
	{
		return front==null && rear==null;
	}
	

}
