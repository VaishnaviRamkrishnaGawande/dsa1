package com.demo.StackList;

public class StackList {
	Node top;
     class Node{
    	 int data;
    	 Node next;
		public Node(int data) {
			super();
			this.data=data;
			next=null;
		}
		public Node() {
			super();
			top=null;
		}
		
		public boolean isEmpty() {
			return top==-1;
			
		}
		
    	 
     }
}
